#include <stdio.h>
#include <stdlib.h>

int main () {
	int i = 1;
	while (i <=5) {
		printf("%d\n",i);
		i++;
	}
	return 0;
}

#include <stdio.h>
#incldue <stdlib.h>
int main () {
	int i;
	for (i = 1; i <= 5; i++) {
		printf("%d\n",i);
}
return 0;	
}


---
//basic form
#include <stdio.h>
#include <stdlib.h>

int main () {
	int luckyNumbers[] = {4,2,31,23,222};
	int i;
	for (i=0; i<5 ;i++) {
		printf("%d\n",luckyNumbers[i]);
		
	} 
	return 0;
}

//anything you can make with for loops can be made with while loops
