#include <stdio.h>
#include <stdlib.h>

int main () {
	char mainCharacterName[] = "John"; //double quotation
	char aA = 'a'; //single quotation around a single chracter
	int a = 35;
	printf("There once was %c man named %s\n",aA,mainCharacterName); 
	printf("He was %d years old.\n",a);
	printf("He really liked the name %s\n",mainCharacterName);
	printf("but did not like being %d.\n",a);
	return 0;
}
