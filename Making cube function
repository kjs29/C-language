
---
//or we can cube it right away
#include <stdio.h>
int main () {
	int cubed;//notice that we don't have []anymore since this is just integer.
	printf("Enter your number and we will cube it : ");
	scanf("%d",&cubed);
	printf("You put %d and the answer is %d",cubed,cubed * cubed * cubed);
	return 0;
}
