#include <stdio.h>
#include <stdlib.h>

int main () {
	char grade = 'B';
	switch (grade) {
		case 'A' :
			printf("You did great!");
			break;
		case 'B' :
			printf("You did nicely");
			break;
		case 'C' :
			printf("Keep working!");
			break;
		case 'D' :
			printf("You can do it!");
			break;
		case 'F' :
			printf("You failed! But it is okay");
			break;
		default :
			printf("INVALID GRADE");
	}
	return 0;
}
